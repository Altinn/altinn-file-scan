{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Function": "Debug",
      "Altinn.FileScan.Functions.FileScanInbound": "Information"
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:00.100",
      "visibilityTimeout": "00:00:01",
      "maxDequeueCount": 5
    }
  }
}